<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: #333333; -fx-border-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="renginiaiFx.Controller">
    <children>
        <Label layoutX="107.0" layoutY="43.0" text="Login" textFill="WHITE">
            <font>
                <Font size="24.0" />
            </font></Label>
        <Label layoutX="20.0" layoutY="102.0" text="Username/Email" textFill="WHITE" />
        <Label layoutX="55.0" layoutY="131.0" text="Password" textFill="WHITE" />
        <TextField fx:id="passwordLogin" layoutX="110.0" layoutY="127.0" style="-fx-border-color: black; -fx-background-color: #333333; -fx-text-fill: white;" />
        <TextField fx:id="usernameEmailLogin" layoutX="110.0" layoutY="97.0" style="-fx-background-color: #333333; -fx-border-color: black; -fx-text-fill: white;" />
        <Button fx:id="loginButton" layoutX="110.0" layoutY="161.0" mnemonicParsing="false" style="-fx-background-color: #333333; -fx-border-color: black;" text="Register" textFill="WHITE" />
        <Label layoutX="83.0" layoutY="310.0" text="Want to create an account?" textFill="WHITE" />
        <Button fx:id="goToRegisterButton" layoutX="115.0" layoutY="336.0" mnemonicParsing="false" onAction="#goToRegister" style="-fx-background-color: #333333; -fx-border-color: black;" text="Click here!" textFill="WHITE" />
    </children>
</AnchorPane>
